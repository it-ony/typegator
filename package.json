{
    "name": "typegator",
    "version": "0.3.0",
    "description": "Navigate web pages, tabs and windows by typing",
    "main": "index.js",
    "scripts": {
        "build": "NODE_ENV=production webpack -p",
        "start": "NODE_ENV=development webpack --watch",
        "package": "rm -f typegator.zip && yarn build && cd dist && zip -r ../typegator.zip * && cd ..",
        "clean": "rimraf dist"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/chrome": "^0.0.96",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.2",
        "extract-loader": "^4.0.3",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^5.1.0",
        "find-replace-webpack-plugin": "^1.0.0",
        "node-sass": "^4.13.1",
        "rimraf": "^3.0.2",
        "sass-loader": "^8.0.2",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.5",
        "webpack": "^4.41.6",
        "webpack-chrome-extension-reloader": "^1.3.0",
        "webpack-cli": "^3.3.11"
    }
}
